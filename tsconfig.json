{
  "compilerOptions": {
    "module": "es2022",
    "moduleResolution": "Node",
    "allowImportingTsExtensions": true,
    "baseUrl": ".",
    "noEmit": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "files": [],
  "references": [
    {
      "path": "./tsconfig.node.json"
    },
    {
      "path": "./tsconfig.app.json"
    }
  ],
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"],
}
